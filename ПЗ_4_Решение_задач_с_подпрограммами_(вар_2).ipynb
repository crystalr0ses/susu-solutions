      "source": [
        "def fibonacci(num):\n",
        "\tfirst, second = 0, 1\n",
        "\tif num == first:\n",
        "\t\treturn first\n",
        "\tif num == second:\n",
        "\t\treturn second\n",
        "\tfor _ in range(1, num):\n",
        "\t\t_next = first + second\n",
        "\t\tfirst = second\n",
        "\t\tsecond = _next\n",
        "\treturn _next\n",
        "\n",
        "number = int(input(''))\n",
        "print(f'FibR({number})={fibonacci(number)}')"
      ]
    }
